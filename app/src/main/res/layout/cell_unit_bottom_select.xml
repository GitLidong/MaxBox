<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cell_unit_select_back"
    android:background="@drawable/size_tool_tab_unselected"
    android:layout_width="76dp"
    android:layout_height="70dp">
    <ImageView
        android:layout_centerHorizontal="true"
        android:background="@drawable/unitconverter_length"
        android:id="@+id/cell_unit_select_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/cell_unit_select_text"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:text="长度"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>
